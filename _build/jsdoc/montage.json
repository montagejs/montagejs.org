{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": [
            "../node_modules/montage/composer/",
            "../node_modules/montage/core/",
            "../node_modules/montage/ui/",
            "../node_modules/montage/window-loader/"
        ],
        "exclude": [],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/)_"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "markdown": {
        "parser": "marked"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "template": "./template",
        "encoding": "utf8",
        "destination": "../../api",
        "recurse": true,
        "tutorials": "./template",
        "query": "value",
        "private": false,
        "lenient": true,
        "githubOrg": "montagejs",
        "githubRepo": "montage",
        "commitish": "v0.13.8"
    },
    "repositoryId": {
        "sha": "11ccc5566986d891716837105385daaaf8884980",
        "message": "Even Rocky had a montage."
    },
    "jsVersion": 180
}